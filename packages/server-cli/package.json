{
  "name": "@mutual8/server-cli",
  "version": "0.0.0",
  "homepage": "https://github.com/mutual8/toolbox/tree/master/packages/server-cli#readme",
  "license": "AGPL-3.0-or-later",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mutual8/toolbox.git"
  },
  "bugs": {
    "url": "https://github.com/mutual8/toolbox/issues"
  },
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run build:lib && npm run build:typeDefs",
    "build:lib": "../../node_modules/.bin/babel -d ./lib -x \".ts,.js\" --ignore \"**/*.spec.ts\" --delete-dir-on-start ./src",
    "build:typeDefs": "../../node_modules/.bin/tsc --noEmit false --allowJs false --emitDeclarationOnly -d --declarationDir ./lib --isolatedModules false",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "dependencies": {
    "@mutual8/cli-logger": "^1.0.0"
  }
}
