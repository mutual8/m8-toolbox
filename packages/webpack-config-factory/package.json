    "postcss-preset-env": "^6.6.0",
    "react-dev-utils": "^9.0.1",
    "react-error-overlay": "^5.1.6",
    "sockjs-client": "^1.3.0",
    "strip-ansi": "^5.2.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack-manifest-plugin": "^2.0.4",
    "webpackbar": "^3.2.0"
  }
